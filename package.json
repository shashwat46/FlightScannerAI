{
  "name": "flightscannerai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:migrate": "supabase db push",
    "db:seed": "npm run db:seed:airlines && npm run db:seed:airports",
    "db:seed:airlines": "psql \"$SUPABASE_DB_URL\" -c \"\\copy airlines from 'supabase/seeds/airlines.csv' csv header\"",
    "db:seed:airports": "psql \"$SUPABASE_DB_URL\" -c \"\\copy airports from 'supabase/seeds/airports.csv' csv header\"",
    "db:reset": "supabase db reset --password $DB_PASS",
    "db:all": "npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@lottiefiles/dotlottie-react": "^0.17.1",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.46.2",
    "amadeus": "^3.3.0",
    "lucide-react": "^0.544.0",
    "next": "^14.2.5",
    "postgres": "^3.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redis": "^4.6.15",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "19.1.13",
    "autoprefixer": "^10.4.20",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "postcss": "^8.4.47",
    "supabase": "^2.40.7",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.4.5"
  }
}
